{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "Node",
        "target": "ES2022",
        "jsx": "react",
        "strictNullChecks": false,
        "strictFunctionTypes": true,
        "sourceMap": true,
        "skipLibCheck": true,
		"rootDir": "src",
		"outDir": "out",
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}